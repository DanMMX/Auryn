{
	"name": "Auryn",
	"version": "2.0.0",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"lint": "node node_modules/eslint/bin/eslint.js --fix .",
		"test": "jest"
	},
	"dependencies": {
		"@youi/react-native-youi": "5.1.1",
		"react": "16.3.0",
		"react-native": "0.55.0",
		"react-navigation": "^2.12.1",
		"react-redux": "^5.0.7",
		"redux": "^4.0.0",
		"redux-debounced": "^0.5.0",
		"redux-promise-middleware": "^5.1.1",
		"redux-thunk": "^2.3.0",
		"youtube-stream-url": "^1.0.1"
	},
	"devDependencies": {
		"@types/jest": "^24.0.13",
		"@types/lodash": "^4.14.128",
		"@types/react": "^16.8.17",
		"@types/react-native": "^0.57.55",
		"@types/react-navigation": "^3.0.7",
		"@types/react-redux": "^7.0.9",
		"@types/react-test-renderer": "^16.8.1",
		"@types/redux": "^3.6.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^23.2.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-react-native": "4.0.0",
		"babel-register": "^6.26.0",
		"eslint": "^5.9.0",
		"eslint-plugin-babel": "^5.2.1",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jest": "^22.3.0",
		"eslint-plugin-react": "^7.11.1",
		"jest": "^23.3.0",
		"react-native-typescript-transformer": "^1.2.12",
		"react-test-renderer": "^16.4.1",
		"ts-jest": "^24.0.2",
		"typescript": "^3.4.5"
	},
	"jest": {
		"preset": "@youi/react-native-youi",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transformIgnorePatterns": [
			"<rootDir>/node_modules/(?!(jest-)?react-native|@youi|react-navigation)"
		],
		"transform": {
			"^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
			"\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		},
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
		"testPathIgnorePatterns": [
			"\\.snap$",
			"<rootDir>/node_modules/"
		],
		"cacheDirectory": ".jest/cache",
		"setupFiles": [
			"./jest-setup.js"
		]
	}
}
